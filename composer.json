{
  "name": "mediamotionag/contao-jwplayer-bundle",
  "homepage": "https://github.com/mediamotionag/contao-jwplayer-bundle",
  "description":"JWPlayer Video-Player",
  "keywords":["contao", "contao4", "contao-bundle", "jwplayer", "video", "element", "background", "fill"],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Media Motion AG",
      "homepage": "https://www.mediamotion.ch/",
      "role": "Developer"
    }
  ],
  "support":{
    "email":"ticket@memo.support",
    "issues":"https://github.com/mediamotionag/contao-jwplayer-bundle/issues",
    "source":"https://github.com/mediamotionag/contao-jwplayer-bundle"
  },
  "require": {
    "php": ">=5.6.0",
    "contao/core-bundle": "^4.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Memo\\ModJWPlayerBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Memo\\ModJWPlayerBundle\\ContaoManager\\Plugin"
  }
}
